<h1>Listing users</h1>
<%= form_tag users_path, method: :get do |f| %>
<%= text_field_tag :q, params[:q] %>
<%= submit_tag :search %>
<% end %>
<table>
  <tr>
    <th>Name</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user['name'] %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
